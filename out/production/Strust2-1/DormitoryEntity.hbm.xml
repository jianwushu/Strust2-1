<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.DormitoryEntity" table="dormitory" schema="struts2">
        <id name="roomId" column="RoomId">
            <generator class="native"></generator>
        </id>
        <property name="dormitoryName" column="DormitoryName" unique-key="unique_key_id"/>
        <property name="roomNumber" column="RoomNumber" unique-key="unique_key_id"/>
    </class>
</hibernate-mapping>