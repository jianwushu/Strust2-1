<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
    <package name="basicAction" extends="struts-default">
    <action name="admin" class="action.LoginAction" method="admin">
        <result name="success">/WEB-INF/pages/admin.jsp</result>
        <result name="error">/WEB-INF/pages/login.jsp</result>
    </action>
    <action name="login" class="action.LoginAction">
        <result name="success">/WEB-INF/pages/admin.jsp</result>
        <result name="error">/WEB-INF/pages/login.jsp</result>
    </action>
</package>
</struts>